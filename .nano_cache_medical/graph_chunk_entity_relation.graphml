      <data key="d2">chunk-61a3433595a4c64ffc02ecb570d32e00</data>
    </node>
    <node id="&quot;EARLY TREATMENT BENEFITS&quot;">
      <data key="d0">"CARE_TIP"</data>
      <data key="d1">"Advice stating that early treatment of strabismus leads to better outcomes."</data>
      <data key="d2">chunk-61a3433595a4c64ffc02ecb570d32e00</data>
    </node>
    <node id="&quot;AMBLYOPIA&quot;">
