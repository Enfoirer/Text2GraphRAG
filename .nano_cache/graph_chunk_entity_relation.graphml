      <data key="d2">chunk-dbc5bf9b1f8e6cc1cba4e23d6687fc24</data>
    </node>
    <node id="&quot;GENETIC PREDISPOSITION&quot;">
      <data key="d0">"RISK_FACTOR"</data>
